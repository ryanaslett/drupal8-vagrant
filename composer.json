{
    "name": "thom8/drupal8-vagrant",
    "description": "Simple Drupal 8 Development Environment",
    "type": "project",
    "license": "MIT",
    "config": {
        "process-timeout": 0
    },
    "minimum-stability": "dev",
    "require-dev": {
        "beet/box": "^0.8"
    },
    "extra": {
        "beetbox": {
            "vagrant_ip": "192.168.88.66",
            "beet_domain": "backdrop.local",
            "beet_root": "{{ beet_base }}/docroot",
            "beet_project": "backdrop",
            "backdrop_checkout": true,
            "backdrop_version": "1.x",
            "backdrop_install_site": true,
            "backdrop_account_name": "admin",
            "backdrop_account_pass": "admin"
        }
    }
}
